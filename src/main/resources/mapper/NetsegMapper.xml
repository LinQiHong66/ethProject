<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.warrior.mapper.NetsegMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.warrior.entity.Netseg">
		<id column="id" property="id" />
		<result column="net_seg" property="netSeg" />
		<result column="start_time" property="startTime" />
		<result column="end_time" property="endTime" />
		<result column="use_time" property="useTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, net_seg AS netSeg, start_time AS startTime, end_time AS endTime, use_time AS useTime
    </sql>

</mapper>
